{
  "expo": {
    "name": "awt3mobile",
    "description": "Awtarika mobile app",
    "slug": "awt3mobile",
    "privacy": "public",
    "sdkVersion": "24.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "icon": "./assets/icons/app-icon.png",
    "loading": {
      "icon": "./assets/icons/loading-icon.png",
      "hideExponentText": false
    },
    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "ios": {
      "supportsTablet": true
    },
    "hooks": {
      "postPublish": [{
        "file": "sentry-expo/upload-sourcemaps",
        "config": {
          "organization": "XXX",
          "project": "XXX",
          "authToken": "XXX"
        }
      }]
    }
  }
}
